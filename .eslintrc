{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": false,
    "presets": [
      "@babel/preset-env"
    ]
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
